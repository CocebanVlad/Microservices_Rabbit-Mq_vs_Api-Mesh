@{
    ViewData["Title"] = "Home Page";
}

@model IndexModel

<form method="POST">
    <div class="form-group">
        <label asp-for="Input"></label>
        <textarea type="text" asp-for="Input" class="form-control" row="10"></textarea>
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Split and Sort</button>
    </div>
</form>

<hr />

<div class="form-group">
    <label asp-for="APIElapsedTime"></label>
    <input type="text" asp-for="APIElapsedTime" class="form-control" readonly />
</div>

<div class="form-group">
    <label asp-for="APIOutput"></label>
    <textarea type="text" asp-for="APIOutput" class="form-control" row="10" readonly></textarea>
</div>

<hr />

<div class="form-group">
    <label asp-for="MQElapsedTime"></label>
    <input type="text" asp-for="MQElapsedTime" class="form-control" readonly />
</div>

<div class="form-group">
    <label asp-for="MQOutput"></label>
    <textarea type="text" asp-for="MQOutput" class="form-control" row="10" readonly></textarea>
</div>